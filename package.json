{
  "name": "sep_smos",
  "version": "1.0.0",
  "description": "Stationary Management and Ordering System Built with the MERN Stack",
  "main": "server.js",
  "scripts": {
    "test": "npm run test-jest && npm run test-cucumber",
    "test-jest": "cd client && jest",
    "test-cucumber": "cd client && ./node_modules/.bin/cucumber-js --require-module babel-register",
    "install-dependencies": "npm install && npm run install-client",
    "install-client": "cd client && npm install",
    "client": "cd client && npm run start",
    "build": "cd client && npm run build",
    "start": "nodemon server.js",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.2.7",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-router-dom": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rishyahluwalia/sep_smos.git"
  },
  "bugs": {
    "url": "https://github.com/rishyahluwalia/sep_smos/issues"
  },
  "homepage": "https://github.com/rishyahluwalia/sep_smos#readme"
}
